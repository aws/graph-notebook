{
  "name": "graph_notebook_widgets",
  "version": "5.0.0",
  "author": "amazon",
  "description": "A Custom Jupyter Library for rendering NetworkX MultiDiGraphs using vis-network",
  "dependencies": {
    "@jupyter-widgets/base": "^6.0.4",
    "feather-icons": "4.28.0",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.13.2",
    "lodash": "4.17.21",
    "vis-data": "7.1.4",
    "vis-network": "9.1.6",
    "vis-util": "5.0.3",
   "@jupyterlab/application": "^4.3.5"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.3.5",
    "@lumino/application": "^2.3.0",
    "@lumino/widgets": "^2.3.0",
    "@types/chai": "4.2.11",
    "@types/feather-icons": "4.7.0",
    "@types/jquery": "^3.5.0",
    "@types/jqueryui": "^1.12.13",
    "@types/json-schema": "^7.0.11",
    "@types/lodash": "4.17.2",
    "@types/mocha": "7.0.2",
    "@types/node": "^20.11.19",
    "@types/webpack-env": "1.15.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "chai": "4.2.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-webpack-plugin": "^3.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fs-extra": "9.0.0",
    "json-schema": "0.4.0",
    "karma": "6.3.16",
    "karma-chai": "0.1.0",
    "karma-firefox-launcher": "1.3.0",
    "karma-mocha": "2.0.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "^5.0.0",
    "mkdirp": "0.5.5",
    "mocha": "7.1.2",
    "npm-run-all": "4.1.5",
    "opencollective": "1.0.3",
    "prettier": "^3.0.0",
    "rimraf": "^3.0.2",
    "sinon": "9.0.2",
    "source-map-loader": "^1.0.2",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-csstree-validator": "^3.0.0",
    "stylelint-prettier": "^4.0.0",
    "ts-loader": "^9.4.4",
    "typescript": "~5.0.4", 
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "@types/backbone": "^1.4.15"
  },
  "files": [
    "lib/**/*.js",
    "dist/*.js",
    "css/*.css"
  ],
  "jupyterlab": {
    "extension": "lib/plugin",
    "outputDir": "labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true,
        "version": "^6.0.4",
        "requiredVersion": "^6.0.4"
      },
      "@jupyterlab/application": {          
        "bundled": false,
        "singleton": true,
        "version": "^4.0.0",
        "requiredVersion": "^4.0.0"
      },
      "@jupyterlab/launcher": {
        "bundled": false,
        "singleton": true,
        "version": "^4.0.0",
        "requiredVersion": "^4.0.0"
      },
      "@jupyterlab/coreutils": {
        "bundled": false,
        "singleton": true,
        "version": "^6.0.0",
        "requiredVersion": "^6.0.0"
      },
      "@jupyterlab/apputils": {
        "bundled": false,
        "singleton": true,
        "version": "^4.0.0",
        "requiredVersion": "^4.0.0"
      },
      "@jupyter/ydoc": {
        "bundled": false,
        "singleton": true,
        "strictVersion": false,
        "version": "^3.0.0",
        "requiredVersion": "^3.0.0"
      }
    }
  },
  "jupyter": {
    "extension": "extension.js", 
    "outputDir": "nbextension"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": { 
    "node": ">=20.18.3",
    "npm": ">=10.8.2"
  },   
  "scripts": {
    "build": "npm run build:lib && npm run build:nbextension && npm run build:labextension:dev",
    "build:prod": "npm run clean && npm run build:lib && npm run build:nbextension && npm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:nbextension": "webpack",
    "clean": "npm run clean:lib && npm run clean:nbextension && npm run clean:labextension",
    "clean:lib": "rimraf lib",
    "clean:labextension": "rimraf labextension",
    "clean:nbextension": "rimraf nbextension",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "prepack": "npm install && npm run build:lib",
    "test": "jest",
    "watch": "npm-run-all -p watch:*",
    "watch:lib": "tsc -w",
    "watch:nbextension": "webpack --watch --mode=development",
    "watch:labextension": "jupyter labextension watch ."
  }
}
